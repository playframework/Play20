. "$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )/scriptLib"

cd ${FRAMEWORK}

if [ "$TRAVIS_EVENT_TYPE" = "cron" ]; then

  printMessage "RUNNING PUBLISH LOCAL"
  runSbt "publishLocal"
  printMessage "PUBLISH LOCAL PASSED"
else
    printMessage "SKIPPING PUBLISH LOCAL"
fi


